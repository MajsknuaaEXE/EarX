  cd /Users/arminjay/Desktop/me/JUCE/Earx/Earx/build-ios
  /opt/homebrew/bin/cmake -E copy /Users/arminjay/Desktop/me/JUCE/Earx/Earx/build-ios/Earx_artefacts/JuceLibraryCode/Earx/PkgInfo /Users/arminjay/Desktop/me/JUCE/Earx/Earx/build-ios/Earx_artefacts/Release/EarX.app
fi
if test "$CONFIGURATION" = "MinSizeRel"; then :
  cd /Users/arminjay/Desktop/me/JUCE/Earx/Earx/build-ios
  /opt/homebrew/bin/cmake -E copy /Users/arminjay/Desktop/me/JUCE/Earx/Earx/build-ios/Earx_artefacts/JuceLibraryCode/Earx/PkgInfo /Users/arminjay/Desktop/me/JUCE/Earx/Earx/build-ios/Earx_artefacts/MinSizeRel/EarX.app
