  cd /Users/arminjay/Desktop/me/JUCE/Earx/Earx/build-ios
  /opt/homebrew/bin/cmake -E copy /Users/arminjay/Desktop/me/JUCE/Earx/Earx/build-ios/Earx_artefacts/JuceLibraryCode/Earx/PkgInfo /Users/arminjay/Desktop/me/JUCE/Earx/Earx/build-ios/Earx_artefacts/RelWithDebInfo/EarX.app
